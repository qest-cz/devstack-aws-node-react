{
    "scripts": {
        "tsc:check": "tsc --noEmit",
        "prettier:check": "prettier -c \"./src/**/*.{ts,tsx}\"",
        "eslint:check": "eslint \"./src/**/*.{ts,tsx}\"",
        "prettier:fix": "prettier --write './**/*.{ts,tsx}'",
        "eslint:fix": "eslint --fix \"./src/**/*.{ts,tsx}\"",
        "code:check": "concurrently -n 'pretty,lint,tsc:check' 'yarn prettier:check' 'yarn eslint:check' 'yarn tsc:check'",
        "code:fix": "eslint --fix '*/**/*.{ts,tsx}'",
        "getenv": "ts-node -T scripts/get-env-vars.ts",
        "dev": "webpack-dev-server",
        "build": "yarn clean && webpack --mode production",
        "build:watch": "yarn clean && webpack --mode production --watch",
        "clean": "concurrently 'rm -rf dist' 'rm -rf coverage'",
        "clean:all": "yarn clean && rm -rf node_modules",
        "test": "jest"
    },
    "name": "fe-showcase",
    "private": true,
    "version": "0.0.1",
    "main": "src/index.js",
    "license": "MIT",
    "devDependencies": {
        "@types/react": "^16.9.55",
        "@types/react-dom": "^16.9.9",
        "eslint": "^7.7.0",
        "eslint-config-airbnb": "^18.2.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.21.5",
        "eslint-plugin-unused-imports": "^0.1.3",
        "prettier": "^2.0.5",
        "ts-jest": "^26.2.0",
        "typescript": "^3.9.7",
        "webpack": "^4.44.1",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0",
        "aws-cdk": "1.67.0",
        "@aws-cdk/aws-s3": "1.67.0",
        "@aws-cdk/aws-s3-deployment": "1.67.0",
        "@aws-cdk/core": "1.67.0"
    },
    "dependencies": {
        "co-utils": "^0.0.1",
        "react": "^17.0.1",
        "react-dom": "^17.0.1"
    }
}
