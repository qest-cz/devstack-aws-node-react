{
    "extends": "../tsconfig",
    "compilerOptions": {
        "target": "es5",
        "lib": ["dom", "dom.iterable", "esnext"],
        "allowJs": false,
        "forceConsistentCasingInFileNames": true,
        "allowSyntheticDefaultImports": true,
        "declaration": false,
        "declarationMap": false,
        "jsx": "react",
        "resolveJsonModule": true,
        "isolatedModules": true
    }
}
