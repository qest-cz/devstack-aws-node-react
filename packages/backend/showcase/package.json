{
    "scripts": {
        "tsc:check": "tsc --noEmit",
        "prettier:check": "prettier -c \"./src/**/*.{ts,tsx}\"",
        "eslint:check": "eslint \"./src/**/*.{ts,tsx}\"",
        "prettier:fix": "prettier --write './**/*.{ts,tsx}'",
        "eslint:fix": "eslint --fix \"./src/**/*.{ts,tsx}\"",
        "code:check": "concurrently -n 'pretty,lint,tsc:check' 'yarn prettier:check' 'yarn eslint:check' 'yarn tsc:check'",
        "code:fix": "concurrently -n 'pretty,lint' 'yarn prettier:fix' 'yarn tslint:fix'",
        "getenv": "awsgetenv",
        "dev": "cross-env $(cat ./env/.env) ts-node -T",
        "dev:server": "nodemon --watch src -e ts  --exec 'yarn dev bin/start-dev-server.ts'",
        "build": "yarn clean && webpack --mode production",
        "build:watch": "yarn clean && webpack --mode production --watch",
        "build:docker": "yarn clean && tsc --outDir dist-docker && cp ./package.json ./dist-docker/package.json && bundle-dependencies ./dist-docker && cd dist-docker && yarn install --production && cp ../docker/distribution/.dockerignore ./.dockerignore && cp ../docker/distribution/Dockerfile ./Dockerfile",
        "clean": "concurrently 'rm -rf ./dist' 'rm -rf ./dist-docker' 'rm -rf ./coverage'",
        "clean:all": "yarn clean && rm -rf node_modules",
        "test": "jest"
    },
    "name": "be-showcase",
    "private": true,
    "version": "0.0.1",
    "main": "index.js",
    "license": "MIT",
